  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc -b && vite build",
    "build:ci": "tsc -b && vite build && npm run check:bundle",
    "check:bundle": "node -e \"const fs=require('fs'),path=require('path');const dir=path.join('dist','assets');if(!fs.existsSync(dir)){console.error('No dist/assets found');process.exit(1)}const files=fs.readdirSync(dir).filter(f=>f.endsWith('.js'));const sizes=files.map(f=>({f, s: fs.statSync(path.join(dir,f)).size}));const limit=600*1024;const offenders=sizes.filter(x=>x.s>limit);if(offenders.length){console.error('Bundle budget exceeded:', offenders);process.exit(1)}else{console.log('Bundle sizes within budget');}\"",
    "lint": "eslint -c eslint.config.js .",
    "test": "vitest",
    "preview": "vite preview"
  },
